{
    "name": "jeffochoa/validator-factory",
    "description": "Standalone library to use Illuminate\\Validation package outside the Laravel framework.",
    "type": "library",
    "version": "1.0.1",
    "require": {
        "illuminate/filesystem": "^8.0",
        "illuminate/translation": "^8.0",
        "illuminate/validation": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jefferson Ochoa (Jeff)"
        }
    ],
    "autoload": {
        "psr-4": {
            "JeffOchoa\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JeffOchoa\\Tests\\": "tests/"
        }
    }
}
